<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soft.ware.rest.modular.goods.dao.TCategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.soft.ware.rest.modular.goods.model.TCategory">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="pid" property="pid" />
        <result column="description" property="description" />
        <result column="sort" property="sort" />
        <result column="is_detele" property="isDetele" />
        <result column="owner_id" property="ownerId" />
        <result column="status" property="status" />
        <result column="is_system" property="isSystem" />
        <result column="category_icon_id" property="categoryIconId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, pid, description, sort, is_detele AS isDetele, owner_id AS ownerId, status, is_system AS isSystem, category_icon_id AS categoryIconId
    </sql>

</mapper>
